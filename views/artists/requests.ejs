<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/styles/main.css" />
    <title>Requested Artist</title>
</head>

<body>
    <header>
        <h1>All Requested Artist</h1>
    </header>
    <nav class=requestsNav>
        <a href="/">Home</a>
        <a href="/artists">All Artist</a>
    </nav>

    <section class=requestedArtist>

        <ul>
            <% artists.forEach(artist=> { %>
                <a href="/artists/requests/<%= artist._id %>">
                    <li>
                        <%= artist.djName %>
                    </li>
                </a>
                <% }); %>
        </ul>

    </section>



    <footer class="subscribeHome">
        <p class="subscribeInfo">Stay up to date with the latest news about Fest.y</p>
        <form action="????????" method='POST'>
            <input class="inputEmail" type="email" name="email" placeholder="Enter Email" />
            <input class="subscribeButton" type="submit" value="Subscribe">
        </form>
    </footer>
</body>

</html>